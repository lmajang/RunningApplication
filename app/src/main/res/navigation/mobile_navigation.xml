<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.runningapplication.runningMain.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_calendar"
        android:name="com.example.runningapplication.runningMain.ui.runningCalendar.calendarFragment"
        android:label="@string/title_calendar"
        tools:layout="@layout/fragment_calendar" />

    <fragment
        android:id="@+id/navigation_friend_list"
        android:name="com.example.runningapplication.runningMain.ui.runningFriendList.friendListFragment"
        android:label="@string/title_friend_list"
        tools:layout="@layout/fragment_friend_list" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.example.runningapplication.runningMain.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications"
        />
</navigation>